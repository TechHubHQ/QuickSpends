{
  "expo": {
    "name": "QuickSpends",
    "slug": "QuickSpends",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/icon-foreground.png",
    "scheme": "quickspends",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quickspends.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.quickspends.app",
      "icon": "./assets/logo/icon-foreground.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/icon-foreground.png",
        "backgroundColor": "#132D51"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo/favicon/favicon-16x16.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#132D51",
          "dark": {
            "backgroundColor": "#132D51"
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f81f6c76-8e79-47db-bbc9-87180c65b898"
      }
    }
  }
}

